CREATE TABLE AQUARIUM (
	AQUARIUM_ID int NOT NULL AUTO_INCREMENT,
	AQUARIUM_NAME varchar(25) NOT NULL,
	AQUARIUM_DESCRIPTION varchar(255),
	PRIMARY KEY (AQUARIUM_ID)
);

CREATE TABLE SENSOR_DATA (
	SENSOR_DATA_ID int NOT NULL AUTO_INCREMENT,
	SENSOR_TYPE varchar(10) NOT NULL,
	SENSOR_ADDRESS int NOT NULL,
	SENSOR_VALUE float NOT NULL,
	AQUARIUM_ID int NOT NULL,
	PRIMARY KEY (SENSOR_DATA_ID),
	FOREIGN KEY (AQUARIUM_ID) REFERENCES AQUARIUM(AQUARIUM_ID)
);

INSERT INTO AQUARIUM(AQUARIUM_NAME, AQUARIUM_DESCRIPTION) 
	VALUES
		("DT", "57 Gallon Display Tank"),
		("QT", "60 Gallon Quarantine Tank");
